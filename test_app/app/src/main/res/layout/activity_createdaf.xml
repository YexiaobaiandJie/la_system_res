<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:padding="3dp">

  <LinearLayout
      android:layout_width="match_parent"
      android:layout_height="25dp"
      android:layout_weight="1"
      android:orientation="horizontal">

    <TextView
        android:id="@+id/textView36"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:text="委托单草稿"
        android:gravity="center"
        android:background="#A9A9A9"
        android:textSize="17dp"/>
  </LinearLayout>

  <LinearLayout
      android:layout_width="match_parent"
      android:layout_height="wrap_content"
      android:layout_weight="12"
      android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="45dp"
        android:orientation="horizontal"
        android:background="#FFEFD5">



      <TextView
          android:id="@+id/textView40"
          android:layout_height="match_parent"
          android:layout_width="0dp"
          android:layout_weight="2.2"
          android:text="项目/合同名称"
          android:gravity="center"
          android:textSize="15dp"/>

      <TextView
          android:id="@+id/textView39"
          android:gravity="center"
          android:layout_width="0dp"
          android:layout_height="match_parent"
          android:layout_weight="1"
          android:text="委托单类型"
          android:textSize="15dp"/>

      <TextView
          android:id="@+id/textView38"
          android:gravity="center"
          android:layout_width="0dp"
          android:layout_height="match_parent"
          android:layout_weight="1.5"
          android:text="保存时间"
          android:textSize="15dp"/>


    </LinearLayout>
    <!--d-->




    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:orientation="horizontal">

      <TextView
          android:id="@+id/textView51"
          android:gravity="center"
          android:layout_height="match_parent"
          android:layout_width="0dp"
          android:layout_weight="2.2"
          android:text="关于信贷基金的贷款收益合同" />

      <TextView
          android:id="@+id/textView52"
          android:gravity="center"
          android:layout_height="match_parent"
          android:layout_width="0dp"
          android:layout_weight="1"
          android:text="审批委托单" />


      <TextView
          android:id="@+id/textView53"
          android:gravity="center"
          android:layout_height="match_parent"
          android:layout_width="0dp"
          android:layout_weight="1.5"
          android:text="2017-01-05" />

    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:orientation="horizontal">

      <TextView
          android:id="@+id/textView56"
          android:gravity="center"
          android:layout_height="match_parent"
          android:layout_width="0dp"
          android:layout_weight="2.2"
          android:text="关于扶贫救困的捐衣法律申请的部分条款" />

      <TextView
          android:id="@+id/textView57"
          android:gravity="center"
          android:layout_height="match_parent"
          android:layout_width="0dp"
          android:layout_weight="1"
          android:text="文书审批单" />

      <TextView
          android:id="@+id/textView55"
          android:gravity="center"
          android:layout_height="match_parent"
          android:layout_width="0dp"
          android:layout_weight="1.5"
          android:text="2016-11-15" />


    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:orientation="horizontal">

      <TextView
          android:id="@+id/textView59"
          android:gravity="center"
          android:layout_height="match_parent"
          android:layout_width="0dp"
          android:layout_weight="2.2"
          android:text="部分以前条款的薪资申请" />

      <TextView
          android:id="@+id/textView61"
          android:gravity="center"
          android:layout_height="match_parent"
          android:layout_width="0dp"
          android:layout_weight="1"
          android:text="单项法人委托" />

      <TextView
          android:id="@+id/textView60"
          android:gravity="center"
          android:layout_height="match_parent"
          android:layout_width="0dp"
          android:layout_weight="1.5"
          android:text="2015-01-12" />


    </LinearLayout>

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:orientation="horizontal"
        >

      <TextView
          android:id="@+id/textView63"
          android:layout_height="match_parent"
          android:layout_width="0dp"
          android:layout_weight="2.2"
          android:text="关于每月团建的支出审核"
          android:gravity="center"/>

      <TextView
          android:id="@+id/textView65"
          android:layout_height="match_parent"
          android:layout_width="0dp"
          android:layout_weight="1"
          android:text="常年法人委托"
          android:gravity="center"/>

      <TextView
          android:id="@+id/textView64"
          android:gravity="center"
          android:layout_height="match_parent"
          android:layout_width="0dp"
          android:layout_weight="1.5"
          android:text="2014-10-25" />


    </LinearLayout>

  </LinearLayout>
</LinearLayout>
